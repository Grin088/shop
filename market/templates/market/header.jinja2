    {% block wrap %}
        <div class="wrap">
    <div class="row Header-rowMain">
        <div class="row-block Header-logo"><a class="logo" href="{{ url('shops:index') }}"><img class="logo-image" src="{{ static('/market/assets/img/logo.png') }}" alt="logo.png" /></a>
        </div>
        <nav class="row-block row-block_right Header-menu">
            <div class="menuModal" id="navigate">
                <ul class="menu menu_main">
                    <li class="menu-item"><a class="menu-link" href="{{ url('shops:index') }}">{% trans %}Главная{% endtrans %}</a>
                    </li>
                    <li class="menu-item"><span class="menu-label menu-label_success">{% trans %}Скидки{% endtrans %}</span><a class="menu-link" href="{{ url('shops:catalog') }}">Каталог</a>
                    </li>
                    <li class="menu-item"><a class="menu-link" href="{{ url('shops:comparison') }}">{% trans %}Сравнение{% endtrans %}</a>
                    </li>
                </ul>
            </div>
        </nav>
        <div class="row-block">

            <div class="CartBlock">

                <div class="dropdown">
                    {% if request.user.is_authenticated  %}
                    <button class="dropbtn"><img class="CartBlock-img" src="{{ static('/market/assets/img/icons/loon-icon.svg') }}" alt="loon-icon.svg" /></button>
                    <div class="dropdown-content">
                        <a class="dropdown-content-a" href="{{ url('users:users_account') }}">{% trans %}Личный кабинет{% endtrans %}</a>
                        <a class="dropdown-content-a" href="{{ url('shops:account') }}">{% trans %}Административный раздел{% endtrans %}</a>
                        <a class="dropdown-content-a" href="{{ url('shops:account') }}">{% trans %}Выход{% endtrans %}</a>
                    </div>
                    {% endif %}
                </div>
                <a class="CartBlock-block" href="{{ url('shops:cart') }}"><img class="CartBlock-img" src="{{ static('/market/assets/img/icons/cart.svg') }}" alt="cart.svg" /><span class="CartBlock-amount">0</span></a>
                <div class="CartBlock-block"><span class="CartBlock-price">0.00$</span>
                </div>
            </div>
        </div>
        <div class="row-block Header-trigger"><a class="menuTrigger" href="#navigate">
                <div class="menuTrigger-content">{% trans %}Показать навигацию{% endtrans %}
                </div><span></span><span></span><span></span>
            </a>
        </div>
    </div>
</div>
    {% endblock %}
